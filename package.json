{
  "name": "enclave",
  "version": "0.8.19",
  "description": "An API for compiling React applications with Webpack",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/eanplatter/enclave.git"
  },
  "scripts": {
    "postinstall": "node src/postinstall/index.js",
    "prestart": "webpack",
    "lint": "eslint *.js src/**/*.js example/**/*.{js,jsx}",
    "install-deps": "npm install --ignore-scripts",
    "example": "webpack-dev-server",
    "test": "npm run lint"
  },
  "pre-commit": [
    "lint"
  ],
  "bin": {
    "enclave": "src/index.js"
  },
  "author": "Ean Platter <eanplatter@gmail.com> (eanplatter.github.io)",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chalk": "^1.1.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.3.0",
    "eslint-config-standard-jsx": "^1.1.1",
    "eslint-plugin-react": "^4.2.1",
    "file-loader": "^0.8.5",
    "history": "^1.13.0",
    "html-webpack-plugin": "^1.6.2",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "ora": "^0.2.0",
    "pre-commit": "^1.1.2",
    "prompt": "^1.0.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "shelljs": "^0.6.0",
    "style-loader": "^0.13.0",
    "url-loader": "0.5.7",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  }
}
